  },
  "main": "./src/main.ts",
  "dependencies": {
    "@electron/remote": "2",
    "debug": "^4.3.4",
    "detect-port": "1",
    "electrom": "19",
    "electron-json-storage-alt": "18",
    "electron-webview-schedule": "18",
    "electron-windows": "18",
    "electron-windows-titlebar": "1",
    "graceful-updater": "1",
    "lodash": "4",
    "lovefield": "^2.1.12",
    "moment": "^2.29.4",
