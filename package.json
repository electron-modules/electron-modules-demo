    "gulp-nodemon": "^2.5.0",
    "husky": "4",
    "lint-staged": "^13.2.1",
    "mocha": "*",
    "monitor.js": "^2.0.1",
    "nyc": "^15.1.0",
    "startserver": "1",
    "ts-jest": "^26.5.6",
    "ts-loader": "^9.2.8",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.3"
  },
  "scripts": {
    "dev": "gulp dev-app",
