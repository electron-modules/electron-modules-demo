    "sql.js": "^1.8.0",
    "windows-verify-trust": "1"
  },
  "optionalDependencies": {
    "@journeyapps/sqlcipher": "^5.3.1"
  },
  "devDependencies": {
    "@applint/spec": "^1.2.3",
    "@electron/asar": "3",
    "@types/jest": "^26.0.24",
    "@types/lodash": "^4.14.181",
    "@types/node": "^18.15.11",
