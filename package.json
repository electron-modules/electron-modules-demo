    "electron-webview-schedule": "18",
    "electron-windows": "18",
    "electron-windows-titlebar": "1",
    "lodash": "4",
    "moment": "^2.29.4",
    "network-interface": "18",
    "semver": "^7.3.8"
  },
  "devDependencies": {
    "@applint/spec": "^1.2.3",
    "@electron/asar": "3",
    "@types/jest": "^26.0.24",
    "@types/lodash": "^4.14.181",
    "cross-env": "^7.0.3",
    "easy-i18n-cli": "1",
    "electron-installer-dmg": "^4.0.0",
    "eslint": "7",
    "eslint-config-egg": "^5.1.1",
    "eslint-plugin-mocha": "^4.11.0",
    "git-contributor": "*",
    "gulp": "^4.0.2",
    "gulp-nodemon": "^2.5.0",
    "husky": "*",
    "mocha": "*",
    "nyc": "^15.1.0",
    "startserver": "1",
    "ts-jest": "^26.5.6",
    "ts-loader": "^9.2.8",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.3"
  },
  "scripts": {
